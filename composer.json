{
    "name": "ws-packages/combination-generate",
    "description": "Generate combinations of items in multiple arrays",
    "version": "1.5.24",
    "type": "library",
    "license": "MIT",
    "keywords": ["php", "laravel", "combinations", "arrays", "variants"],
    "autoload": {
        "psr-4": {
            "WsPackages\\CombinationGenerate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WsPackages\\CombinationGenerate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "An Engineer",
            "email": "admin@google.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WsPackages\\CombinationGenerate\\Providers\\CombinationServiceProvider"
            ],
            "aliases": {
                "Combination": "WsPackages\\CombinationGenerate\\Facades\\Combination"
            }
        }
    }
}
